   <rdf:Description rdf:about="#OrientationKind.negative">
      <cims:stereotype>enum</cims:stereotype>
      <rdf:type rdf:resource="#OrientationKind"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">For 2D diagrams, a negative orientation gives the left-hand orientation (favoured by computer graphics displays) with X values increasing from left to right and Y values increasing from top to bottom.   This is also known as a left hand orientation.</rdfs:comment>
      <rdfs:label xml:lang="en">negative</rdfs:label>
   </rdf:Description>
